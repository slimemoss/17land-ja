{
  "extends": "plasmo/templates/tsconfig.base",
  "exclude": [
    "node_modules"
  ],
  "include": [
    ".plasmo/index.d.ts",
    "./**/*.ts",
    "./**/*.tsx"
  ],
  "compilerOptions": {
    "target": "ES2023",
    "lib": ["ES2023", "DOM"],
    "paths": {
      "~*": [
        "./*"
      ]
    },
    "baseUrl": "."
  }
}
